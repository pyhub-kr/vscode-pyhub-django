{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "skip-full": true,
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/**/*",
    "out/**/*",
    "**/*.d.ts"
  ],
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}