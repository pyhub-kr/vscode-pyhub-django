{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "skip-full": true,
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "include": [
    "out/**/*.js"
  ],
  "exclude": [
    "out/test/**/*",
    "out/extension.js",
    "out/extension.old.js",
    "**/*.d.ts",
    "**/*.test.js"
  ],
  "lines": 0,
  "functions": 0,
  "branches": 0,
  "statements": 0
}